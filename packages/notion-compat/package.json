{
  "name": "@hopeness.com/notion-compat",
  "private": false,
  "version": "6.16.1",
  "type": "module",
  "description": "Compatibility layer between the official Notion API and unofficial private API.",
  "repository": "hopeness-com/react-notion-x",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "@hopeness.com/notion-types": "^6.16.1",
    "@hopeness.com/notion-utils": "^6.16.1",
    "p-queue": "^7.4.1"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.13",
    "@hopeness.com/notion-client": "^6.16.1"
  },
  "peerDependencies": {
    "@notionhq/client": "^2.2.13"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  }
}
